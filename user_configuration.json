{
    "additional-repositories": [
        "multilib"
    ],
    "archinstall-language": "English",
    "audio_config": {
        "audio": "pulseaudio"
    },
    "bootloader": "Systemd-boot",
    "config_version": "2.7.1",
    "debug": true,
    "disk_config": {
        "config_type": "default_layout",
        "device_modifications": [
            {
                "device": "/dev/vda",
                "partitions": [
                    {   
                        "boot": true,
                        "dev_path": null,
                        "flags": ["Boot"],
                        "fs_type": "fat32",
                        "mount_options": [],
                        "mountpoint": "/boot",
                        "size": {
                            "sector_size": {
                                "unit": "B",
                                "value": 512
                            },
                            "unit": "MiB",
                            "value": 1024
                        },
                        "start": {
                            "sector_size": {
                                "unit": "B",
                                "value": 512
                            },
                            "unit": "MiB",
                            "value": 5
                        },
                        "status": "create",
                        "type": "primary"
                    },
                    {
                        "btrfs": [
                            {
                                "compress": false,
                                "mountpoint": "/",
                                "name": "@",
                                "nodatacow": false
                            },
                            {
                                "compress": false,
                                "mountpoint": "/home",
                                "name": "@home",
                                "nodatacow": false
                            },
                            {
                                "compress": false,
                                "mountpoint": "/var/log",
                                "name": "@log",
                                "nodatacow": false
                            },
                            {
                                "compress": false,
                                "mountpoint": "/var/cache/pacman/pkg",
                                "name": "@pkg",
                                "nodatacow": false
                            },
                            {
                                "compress": false,
                                "mountpoint": "/.snapshots",
                                "name": "@.snapshots",
                                "nodatacow": false
                            }
                        ],
                        "dev_path": null,
                        "fs_type": "btrfs",
                        "mount_options": [
                            "compress=zstd"
                        ],
                        "size": {
                            "sector_size": {
                                "unit": "B",
                                "value": 512
                            },
                            "unit": "Percent",
                            "value": 100
                        },
                        "start": {
                            "sector_size": {
                                "unit": "B",
                                "value": 512
                            },
                            "unit": "MiB",
                            "value": 1029
                        },
                        "status": "create",
                        "type": "primary"
                    }
                ],
                "wipe": true
            }
        ]
    },
    "disk_encryption": {
        "encryption_type": "luks",
        "partitions": []
    },
    "hostname": "archlinux",
    "kernels": [
        "linux",
        "linux-lts"
    ],
    "locale_config": {
        "kb_layout": "br-abnt2",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "mirror_config": {
        "custom_mirrors": [],
        "mirror_regions": "Worldwide"
    },
    "network_config": {
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "ntp": true,
    "offline": false,
    "parallel downloads": 10,
    "packages": ["docker", "git", "wget", "zsh"],
    "services": ["docker"],
    "custom-commands": [
        "cd /home/devel; git clone https://aur.archlinux.org/paru.git",
        "chown -R devel:devel /home/devel/paru",
        "usermod -aG docker devel",
        "chsh -s $(which zsh)"
    ],
    "profile_config": {
        "gfx_driver": "All open-source",
        "greeter": "sddm",
        "profile": {
            "custom_settings": {
                "Kde": {},
                "Hyprland": {
                    "seat_access": "polkit"
                }
            },
            "details": [
                "Kde",
                "Hyprland"
            ],
            "main": "Desktop"
        }
    },
    "script": "guided",
    "silent": false,
    "skip_ntp": false,
    "skip_version_check": false,
    "swap": true,
    "timezone": "America/Sao_Paulo",
    "uki": false,
    "version": "2.7.1"
}