{
    "additional-repositories": [
        "multilib"
    ],
    "archinstall-language": "English",
    "audio_config": {
        "audio": "pulseaudio"
    },
    "bootloader": "Systemd-boot",
    "config_version": "2.7.1",
    "debug": true,
    "harddrives": ["nvme0n1"],
    "disk_config": null,
    "disk_encryption": {
        "encryption_type": "luks",
        "partitions": []
    },
    "hostname": "archlinux",
    "kernels": [
        "linux",
        "linux-lts"
    ],
    "locale_config": {
        "kb_layout": "br-abnt2",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "mirror_config": {
        "custom_mirrors": [],
        "mirror_regions": "Worldwide"
    },
    "network_config": {
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "ntp": true,
    "offline": false,
    "parallel downloads": 10,
    "packages": ["docker", "git", "wget", "zsh"],
    "services": ["docker"],
    "custom-commands": [
        "cd /home/devel; git clone https://aur.archlinux.org/paru.git",
        "chown -R devel:devel /home/devel/paru",
        "usermod -aG docker devel",
        "chsh -s $(which zsh)"
    ],
    "profile_config": {
        "gfx_driver": "All open-source",
        "greeter": "sddm",
        "profile": {
            "custom_settings": {
                "Kde": {},
                "Hyprland": {
                    "seat_access": "polkit"
                }
            },
            "details": [
                "Kde",
                "Hyprland"
            ],
            "main": "Desktop"
        }
    },
    "script": "guided",
    "silent": false,
    "skip_ntp": false,
    "skip_version_check": false,
    "swap": true,
    "timezone": "America/Sao_Paulo",
    "uki": false,
    "version": "2.7.1"
}